{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter Web (No CORS)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "-d",
        "chrome",
        "--web-browser-flag=--disable-web-security",
        "--web-renderer",
        "html"
      ]
    }
  ]
}